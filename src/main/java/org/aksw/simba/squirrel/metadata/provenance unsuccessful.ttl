@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix prov:     <http://www.w3.org/ns/prov-o/> .
@prefix:          <http://sample.com/> .

:crawlingActivity_1
    a prov:Activity;
    dcterms:description "crawling activity of a worker.";
    prov:wasAssociatedWith :Worker_1;
    prov:startedAtTime "2012-04-30T20:40:40"^^xsd:dateTime;
    prov:endedAtTime "2012-04-30T20:40:40"^^xsd:dateTime;
    prov:qualifiedAssociation [
      a prov:Association;
      prov:agent :worker_ID;
      rdfs:comment "Worker had crawling plan"@en;
      prov:hadPlan :crawling;
   ];
    rdfs:Comment "Sink used to store the Content(metadata and data) of URI collected"@en;
    sq:numberOfTriples "number of triples";
    sq:hostedOn "LocalHost:3030"
    prov:wasAssociatedWith :Worker_ID;
    sq:status "unsuccessfull";
.

:worker_ID
    a prov:Agent,Prov:Association;       ## Association with CrawlingActivity
    rdfs:comment "Worker_Id collects URI, Unstructured data and Meta Data"@en;;
.

:crawling
    a prov:Plan, prov:Entity;  ## A plan is an entity that represents a set of actions or steps intended by one or more agents to achieve some goals.
    rdfs:comment "steps: http fetcher, rdf analyzer, sink"@en;
.

:resultgraph
    a prov:Entity;
    prov:wasGeneratedBy :crawlingActivity_1;
    sq:uriName uri;
.

:Uri
a prov:Entity;
.



